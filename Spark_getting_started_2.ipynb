   "source": [
    "from pyspark.sql import SparkSession\n",
    "\n",
    "spark = SparkSession.builder.master(\"local[*]\").getOrCreate()\n",
    "\n",
    "sc = spark.sparkContext\n",
    "\n",
    "# Create an RDD\n",
    "rdd = sc.parallelize(range(1000000000000000000), 10)\n",
    "rdd.take(10)\n"
